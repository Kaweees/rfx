{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://rfx.dev/schemas/eval_report.schema.json",
  "title": "rfx Eval Report",
  "type": "object",
  "required": [
    "schema_version",
    "run_id",
    "artifact_ref",
    "metrics",
    "passed"
  ],
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "run_id": {
      "type": "string"
    },
    "artifact_ref": {
      "type": "string"
    },
    "generated_at": {
      "type": "string"
    },
    "metrics": {
      "type": "object"
    },
    "thresholds": {
      "type": "object"
    },
    "passed": {
      "type": "boolean"
    }
  },
  "additionalProperties": true
}
